{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "baseUrl": "./src",
    "checkJs": true,
    "paths": {
      "@/components/*": [
        "presentation/components/*"
      ],
      "@/screens/*": [
        "presentation/screens/*"
      ],
      "@/routes/*": [
        "presentation/routes/*"
      ]
    },
  },
  "include": [
    "./src",
    "index.js"
  ],
  "exclude": [
    "**/node_modules",
    "**/Pods"
  ]
}
